{
  "function": {
    "SendSMSSPCreateAuthChallenge": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "SendSMSSPDefineAuthChallenge": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "SendSMSSPPreSignup": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "SendSMSSPVerifyAuthChallengeResponse": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "auth": {
    "SendSMSSP": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "SendSMSSPCreateAuthChallenge",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "SendSMSSPDefineAuthChallenge",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "SendSMSSPPreSignup",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        },
        {
          "category": "function",
          "resourceName": "SendSMSSPVerifyAuthChallengeResponse",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": true,
      "frontendAuthConfig": {
        "socialProviders": [],
        "usernameAttributes": [],
        "signupAttributes": [
          "PHONE_NUMBER"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 8,
          "passwordPolicyCharacters": []
        },
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "verificationMechanisms": [
          "PHONE_NUMBER"
        ]
      }
    }
  }
}